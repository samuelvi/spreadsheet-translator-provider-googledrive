{
    "name": "samuelvi/spreadsheet-translator-provider-googledrive",
    "license": "MIT",
    "type": "library",
    "description": "Spreadsheet Translator - Google Drive Provider with no authentication",
    "autoload": {
        "psr-4": {
            "Atico\\SpreadsheetTranslator\\Provider\\GoogleDrive\\": "./src"
        }
    },
    "repositories": [{
        "type": "path",
        "url": "../../Core",
        "options": {
            "symlink": true
        }
    }],
    "require": {
        "samuelvi/spreadsheet-translator-core": "^1.0|^2.0|^8.0",
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.7"
    },
    "require-dev": {
        "rector/jack": "^0.2.9",
        "rector/rector": "^2.1"
    }
}
